<!DOCTYPE html>
<html lang="ru"><head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}BigWine{% endblock %}</title>
    {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('/bigwine/css/style.css?ver=750') }}">
    <link rel="stylesheet" href="{{ asset('/bigwine/css/tuning.css') }}">
    {% endblock %}
    <meta name="format-detection" content="telephone=no">
    <meta name="robots" content="noindex, nofollow"/>
</head>

<body>
<div class="wrapper-def">
    <div class="mob-menu">
        {% include 'front/common/mob_menu.html.twig' %}
    </div>

    <header class="top-menu">
        {% include 'front/common/slider.html.twig' %}
    </header>

    <section class="sec-slider">
    </section>

    <div class="sec-info">
        {% block body %}
        {% endblock %}
    </div>

    <div class="sec-events">
    </div>

    <footer class="sec-footer">
        {% include 'front/common/footer.html.twig' %}
    </footer>

</div>

<script src="{{ asset('/bigwine/js/main.js?ver=750') }}"></script>

{% block extra_scripts %}
    <script src="{{ asset('js/jquery.min.js') }}"></script>
    <script src="{{ asset('js/jquery.easing.min.js') }}"></script>
    <script src="{{ asset('js/jquery.fancybox.min.js') }}"></script>
    <script src="{{ asset('js/flash-messages.min.js') }}"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.form/4.3.0/jquery.form.min.js" integrity="sha384-qlmct0AOBiA2VPZkMY3+2WqkHtIQ9lSdAsAn5RUJD/3vA5MKDgSGcdmIv4ycVxyn" crossorigin="anonymous"></script>
{% endblock %}
</body>
</html>

