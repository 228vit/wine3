<div class="container-def">
    <div class="top-menu__wrap">
        <div class="top-menu__content"><a class="top-menu__logo logo-def" href="#"><img
                        class="logo-def__img"
                        src="{{ asset('/bigwine/img/logo.svg') }}" alt=""></a>
            <nav class="top-menu__nav">
                <div class="top-menu__lang lang-choose"><a class="lang-choose__el" href="#"><img
                                src="{{ asset('/bigwine/img/ru.svg') }}" alt=""></a></div>
                <ul class="links-def top-menu__links">
                    <li class="links-def__el"><a
                            class="links-def__link {{ app.request.get('_route') == 'homepage' ? 'active' : '' }}"
                            href="{{ path('homepage') }}">Главная</a>
                    </li>
                    <li class="links-def__el"><a
                            class="links-def__link {{ app.request.get('_route') == 'front_catalog' ? 'active' : '' }}"
                            href="{{ path('front_catalog') }}">Каталог</a>
                    </li>
                    <li class="links-def__el"><a
                            class="links-def__link {{ app.request.get('_route') == 'front_events_calendar' ? 'active' : '' }}"
                            href="{{ path('front_events_calendar') }}">Мероприятия</a>
                    </li>
                    <li class="links-def__el"><a class="links-def__link" href="#">Поставщики</a></li>
                    <li class="links-def__el"><a class="links-def__link" href="#">Сотрудничество</a></li>
                </ul>
            </nav>
            {% if app.user %}
                <div class="top-menu__profile profile-min"><a
                            class="active profile-min__act profile-min-act"
                            href="{{ path('cabinet_product_index') }}">
                        <div class="profile-min-act__count">6</div>
                        <svg width="21" height="23" viewBox="0 0 21 23" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M14.0625 17.3333H20L18.3316 15.6942C17.8792 15.2497 17.625 14.6468 17.625 14.0182V10.3333C17.625 7.28549 15.6423 4.6926 12.875 3.73165V3.33333C12.875 2.04467 11.8117 1 10.5 1C9.18832 1 8.125 2.04467 8.125 3.33333V3.73165C5.35767 4.6926 3.375 7.28549 3.375 10.3333V14.0182C3.375 14.6468 3.12081 15.2497 2.66836 15.6942L1 17.3333H6.9375M14.0625 17.3333V18.5C14.0625 20.433 12.4675 22 10.5 22C8.53249 22 6.9375 20.433 6.9375 18.5V17.3333M14.0625 17.3333H6.9375" stroke="#A1ACB8" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path>
                        </svg>
                    </a><a class="profile-min__act profile-min-act" href="#">
                        <svg width="24" height="21" viewBox="0 0 24 21" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M2.61091 2.62994C0.463029 4.80319 0.463029 8.32672 2.61091 10.5L12.0001 20L21.3891 10.5C23.537 8.32672 23.537 4.80318 21.3891 2.62994C19.2412 0.456688 15.7588 0.456688 13.6109 2.62994L12.0001 4.25994L10.3891 2.62994C8.2412 0.456688 4.7588 0.456688 2.61091 2.62994Z" stroke="#A1ACB8" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path>
                        </svg>
                    </a><a class="profile-min-user" href="#"><img class="profile-min-user__img" src="{{ asset('/bigwine/img/user.png') }}" alt="">
                        <div class="profile-min-user__name">{{ app.user.email }}</div>
                    </a><a class="profile-min__exit" href="{{ path('app_logout') }}">
                        <svg width="18" height="17" viewBox="0 0 18 17" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M12.3816 12.5L16.1647 8.75M16.1647 8.75L12.3816 5M16.1647 8.75H6.70703" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path>
                            <path d="M8.72461 1H4.18495C3.1256 1 2.59592 1 2.1913 1.20436C1.83539 1.38413 1.54602 1.67096 1.36467 2.02377C1.15851 2.42485 1.15851 2.9499 1.15851 4V13C1.15851 14.0501 1.15851 14.5751 1.36467 14.9762C1.54602 15.329 1.83539 15.6159 2.1913 15.7956C2.59592 16 3.1256 16 4.18495 16H8.72461" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path>
                        </svg>
                    </a></div>

            {% else %}
                <a id="cabinetLink" class="top-menu__link link-lk" href="#">
                    <svg class="link-lk__icon" width="17" height="17" viewBox="0 0 17 17" fill="none"
                         xmlns="http://www.w3.org/2000/svg">
                        <path d="M6.625 12.25L10.375 8.5M10.375 8.5L6.625 4.75M10.375 8.5H1M8.5 1H13C14.0501 1 14.5751 1 14.9762 1.20436C15.329 1.38413 15.6159 1.67096 15.7956 2.02377C16 2.42485 16 2.9499 16 4V13C16 14.0501 16 14.5751 15.7956 14.9762C15.6159 15.329 15.329 15.6159 14.9762 15.7956C14.5751 16 14.0501 16 13 16H8.5"
                              stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                    <span>Личный кабинет</span>
                </a>
            {% endif %}
            <div class="top-menu__burger btn-burger">
                <svg width="33" height="25" viewBox="0 0 33 25" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <rect width="33" height="3" rx="1.5" fill="#0842B4"/>
                    <rect y="11" width="33" height="3" rx="1.5" fill="#0842B4"/>
                    <rect y="22" width="33" height="3" rx="1.5" fill="#0842B4"/>
                </svg>
            </div>
        </div>
    </div>
</div>
