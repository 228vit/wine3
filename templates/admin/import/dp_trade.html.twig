            <tr>
                <td>{{ id }}</td>
                <td>{{ name }}</td>
            </tr>
        {% endfor %}

    </table>
    <table class="table table-striped table-bordered">
    {% for product in products %}
        <tr>
            <td>{{ product.id }}</td>
